-- Scripts:

local function NTGSKWV_fake_script() -- MainTabButton.LocalScript 
	local script = Instance.new('LocalScript', MainTabButton)

	local mouse = script.Parent
	local ScriptsTab = script.Parent.Parent.Parent.Parent.MainFrame.ScriptsTab
	local CreditsTab = script.Parent.Parent.Parent.Parent.MainFrame.CreditsTab
	local MainTab = script.Parent.Parent.Parent.Parent.MainFrame.MainTab
	local VisualsTab = script.Parent.Parent.Parent.Parent.MainFrame.VisualsTab
	local AddonsTab = script.Parent.Parent.Parent.Parent.MainFrame.AddonsTab
	
	mouse.MouseButton1Click:Connect(function()
		ScriptsTab.Visible = false
		CreditsTab.Visible = false
		MainTab.Visible = true
		VisualsTab.Visible = false
		AddonsTab.Visible = false
	end)
end
coroutine.wrap(NTGSKWV_fake_script)()
