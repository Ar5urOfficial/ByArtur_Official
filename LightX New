local function PSXO_fake_script() -- EnterNumbers.LocalScript 
	local script = Instance.new('LocalScript', EnterNumbers)

	wait(2)
	local plr = game.Players.LocalPlayer
	local char = plr.Character
	local hum = char:WaitForChild("Humanoid")
	
	local limit = 75
	
	script.Parent.Changed:Connect(function()
		hum.WalkSpeed = script.Parent.Text
		
		if hum.WalkSpeed >= limit then
			script.Parent.Text = "75"
		end
	end)
end
coroutine.wrap(PSXO_fake_script)()
local function JUJBE_fake_script() -- SaveSettings.LocalScript 
	local script = Instance.new('LocalScript', SaveSettings)

	local cam = game.Workspace.Camera
	local player = game.Players.LocalPlayer
	local button = script.Parent
	
	local userGameSettings = UserSettings():GetService("UserGameSettings")
	local mouseSensitivitySetting = userGameSettings.MouseSensitivity
	
	local function onActivated()
		cam.FieldOfView = player.PlayerGui.LightX.MainFrame.VisualsTab.FovChanger.EnterNumbers.Text
		player.PlayerGui.LightX.MainFrame.Visible = false
	end
	
	button.Activated:Connect(onActivated)
end
coroutine.wrap(JUJBE_fake_script)()
local function PZXI_fake_script() -- OpenFrame.LocalScript 
	local script = Instance.new('LocalScript', OpenFrame)

	script.Parent.Parent.MainFrame.Visible = false
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.MainFrame.Visible = true
	end)
end
coroutine.wrap(PZXI_fake_script)()
